{
    "gpu": {
        "use": true,
        "id": 0
    },


    "params": {
        "seed": 9,
        "epochs": 50,
        "runs": 5,
        "batch_size": 64,
        "lr": 0.001,
        "weight_decay": 0,
        "epsilon": 1.0e-3,
        "clip": 5,
        "print_every": 50,
        "dataset": "solar",
        "out_dir": "save_gru"
    },

    "net_params": {
        "model": "gru",
        "in_dim": 1,
        "dim": 64,
        "out_dim": 1,
        "num_layers": 2,
        "num_nodes": 137,
        "dropout": 0,
        "seq_in_len": 12,
        "seq_out_len": 12,
        "filter_type": "dual_random_walk",
        "use_curriculum_learning": true,
        "cl_decay_steps": 2000,
        "max_diffusion_step": 2
    }
}

